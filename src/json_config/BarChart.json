{
     
    "chart": {
        "id": "vuechart-example",
        "background": "#000",
        "foreColor": "#FFF",
        "toolbar": {
            "show": true,
            "offsetX": 0,
            "offsetY": 0,
            "tools": {
                "download": true,
                "selection": true,
                "zoom": true,
                "zoomin": true,
                "zoomout": true,
                "pan": true
            }
        }
    },
    "xaxis": {
        "type":"string",
        "categories": ["17:30", "17:35", "17:40", "17:45", "17:50", "17:55", "18:00"],
        "color": ["#FFF"]
    },
    "colors":["#ffff00", "#00e00e", "#ff0000", "#bfbfbf", "#00aeef"],
    "fill": {
        "colors": ["#ffff00", "#00e00e", "#ff0000", "#bfbfbf", "#00aeef"]
    },
    "dataLabels": {
          "style": {
            "colors": ["#FFF"]
        }
    }

}
